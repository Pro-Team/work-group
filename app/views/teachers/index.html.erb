<p id="notice"><%= notice %></p>

<h1>Teachers</h1>

<table>
  <thead>
    <tr>
      <th>Fist name</th>
      <th>Last name</th>
      <th>Gender</th>
      <th>Maritus status</th>
      <th>Age</th>
      <th>Dateofbirthday</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @teachers.each do |teacher| %>
      <tr>
        <td><%= teacher.fist_name %></td>
        <td><%= teacher.last_name %></td>
        <td><%= teacher.gender %></td>
        <td><%= teacher.maritus_status %></td>
        <td><%= teacher.age %></td>
        <td><%= teacher.dateOfBirthday %></td>
        <td><%= link_to 'Show', teacher %></td>
        <td><%= link_to 'Edit', edit_teacher_path(teacher) %></td>
        <td><%= link_to 'Destroy', teacher, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Teacher', new_teacher_path %>
